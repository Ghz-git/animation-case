{"file":"index-1lBQMZcY.js","mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiBI,UAAM,QAAQ;AACd,UAAM,cAAc,SAAS,MAAM;AAC/B,aAAO,MAAM,MAAM,MAAM,CAAC,MAAM,MAAM,MAAM,MAAM,KAAK;AAAA,IAC/D,CAAK;AACD,UAAM,EAAE,MAAM;;;;;;;;;;;;;;;;;","names":[],"sources":["../../../../src/layouts/components/triggerBtn/index.vue"],"sourcesContent":["<template>\r\n    <div class=\"trigger-btns_wrap\">\r\n        <Transition name=\"fade\">\r\n            <div class=\"trigger-btns\">\r\n                <button class=\"btn\" :class=\"{ acitve: activeIndex === idx }\" @click=\"triggerHandler(item, idx)\" v-for=\"(item, idx) in ACADEMY_LIST\" :key=\"idx\">{{ t(item.label) }}</button>\r\n            </div>\r\n        </Transition>\r\n        <!-- <div class=\"search\">\r\n            <input class=\"in\" v-model=\"inputVal\" @input=\"searchHandler\" :placeholder=\"t('Login.placeholder6')\" />\r\n            <i class=\"iconfont icon-sousuo\"></i>\r\n        </div> -->\r\n    </div>\r\n</template>\r\n<script setup>\r\n    import { ACADEMY_LIST } from \"@/data/academy\";\r\n    import { nuxtTo, getQuery } from \"@/utils/tools\";\r\n    import { ref } from \"vue\";\r\n    const route = useRoute();\r\n    const activeIndex = computed(() => {\r\n        return route.query.key ? +route.query.key : route.meta.index;\r\n    });\r\n    const { t } = useI18n();\r\n    // defineProps({\r\n    //     btnList: {\r\n    //         type: Array,\r\n    //         default: []\r\n    //     }\r\n    // });\r\n    const emits = defineEmits([\"trigger\"]);\r\n    const linkTo = url => {\r\n        let query = url.includes(\"?\") ? getQuery(url) : {};\r\n        nuxtTo(url, query);\r\n    };\r\n    const triggerHandler = (item, idx) => {\r\n        emits(\"trigger\", idx);\r\n        linkTo(item.path);\r\n    };\r\n</script>\r\n<style scoped lang=\"scss\" src=\"./index.scss\"></style>\r\n"],"version":3}