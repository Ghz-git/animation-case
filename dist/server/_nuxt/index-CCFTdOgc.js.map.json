{"file":"index-CCFTdOgc.js","mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwDM,UAAA,EAAE,MAAM;AAIQ,QAAI,CAAC;;;;;;;;;;;;;;;;;;;","names":[],"sources":["../../../../src/pages/index/components/partner/index.vue"],"sourcesContent":["<template>\r\n    <div class=\"partner\" :data-title=\"t('Home.partner.title')\">\r\n<!--        <img src=\"@/assets/images/page/bg.png\" alt=\"\" class=\"partner-bg\">-->\r\n        <div class=\"partner-inner\">\r\n            <div class=\"title\">{{ t(\"Home.partner.title\") }}</div>\r\n            <div class=\"list\">\r\n                <client-only>\r\n                <swiper\r\n                    :spaceBetween=\"15\"\r\n                    :slidesPerView=\"slidesPerView\"\r\n                    :speed=\"4000\"\r\n                    :loop=\"true\"\r\n                    :freeMode=\"true\"\r\n                    :autoplay=\"{\r\n                        delay: 0,\r\n                        disableOnInteraction: false\r\n                    }\"\r\n                    :modules=\"modules\"\r\n                    class=\"mySwiper\"\r\n                >\r\n                    <swiper-slide v-for=\"(path, idx) in listTop\" :key=\"idx\">\r\n                        <img :src=\"path\" class=\"partner-logo\"  alt=\"\"/>\r\n                    </swiper-slide>\r\n                </swiper>\r\n                <swiper\r\n                    :spaceBetween=\"15\"\r\n                    :slidesPerView=\"slidesPerView\"\r\n                    :speed=\"4000\"\r\n                    :loop=\"true\"\r\n                    :freeMode=\"true\"\r\n                    :autoplay=\"{\r\n                        delay: 0,\r\n                        disableOnInteraction: false,\r\n                        reverseDirection: true\r\n                    }\"\r\n                    :modules=\"modules\"\r\n                    class=\"mySwiper\"\r\n                >\r\n                    <swiper-slide v-for=\"(path, idx) in listBottom\" :key=\"idx\">\r\n                        <img :src=\"path\" class=\"partner-logo\"  alt=\"\"/>\r\n                    </swiper-slide>\r\n                </swiper>\r\n            </client-only>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n</template>\r\n<script setup>\r\nimport {onMounted, ref} from \"vue\";\r\nimport { PARTNER_LIST } from \"@/data/image.js\"\r\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\r\nimport { FreeMode, Autoplay } from \"swiper/modules\";\r\nimport \"swiper/scss\";\r\nimport \"swiper/scss/free-mode\";\r\nconst modules = [FreeMode, Autoplay];\r\nconst { t } = useI18n();\r\n\r\nconst partnerList = PARTNER_LIST()\r\n// 每页数量\r\nconst slidesPerView = ref(7)\r\n\r\n\r\n// 将数据分为两部分\r\nconst middleIndex = Math.ceil(partnerList.length / 2);\r\nconst listTop = partnerList.splice(0, middleIndex);\r\nconst listBottom = partnerList.splice(-middleIndex);\r\n\r\nif(listTop.length<=11){\r\n    listTop.push(...listTop)\r\n}\r\nif(listBottom.length<=11){\r\n    listBottom.push(...listBottom)\r\n}\r\n\r\n// 通过尺寸 设置显示个数\r\nconst setPerViewNum = ()=>{\r\n\r\n    if(window.innerWidth>1024){\r\n        slidesPerView.value = 7\r\n    }else{\r\n        slidesPerView.value = 4\r\n    }\r\n}\r\n\r\nonMounted(()=>{\r\n    setPerViewNum()\r\n    window.onresize=()=>{\r\n        setPerViewNum()\r\n    }\r\n\r\n})\r\n</script>\r\n<style scoped lang='scss' src=\"./index.scss\"></style>"],"version":3}